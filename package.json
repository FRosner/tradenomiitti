{
  "name": "tradenomiitti",
  "version": "1.0.0",
  "description": "Löydä elämäsi tradenomi",
  "main": "index.js",
  "scripts": {
    "preansible": "./conf/preinstall.sh",
    "serverinstallbase": "./conf/serverinstallbase.sh",
    "serverinstallpm2": "ansible-playbook conf/pm2install.yml -i conf/inventory.ini",
    "compilelocal": "cd frontend && mkdir -p static && elm-make --yes Main.elm --output static/main.js && ../node_modules/.bin/node-sass -r stylesheets/ -o static/",
    "start": "environment=local node --harmony server/index.js",
    "compilefrontend": "cd frontend && elm-make --yes Main.elm --output /srv/static/main.js",
    "compilescss": "node-sass -r frontend/stylesheets/ -o /srv/static/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurice/tradenomiitti.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/futurice/tradenomiitti/issues"
  },
  "homepage": "https://github.com/futurice/tradenomiitti#readme",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-session": "^2.0.0-alpha.3",
    "elm": "^0.18.0",
    "express": "^4.14.1",
    "knex": "^0.12.6",
    "node-sass": "^4.5.0",
    "pg": "^6.1.2",
    "request": "^2.79.0",
    "sqlite3": "^3.1.8",
    "uuid": "^3.0.1"
  }
}
